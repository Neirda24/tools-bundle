<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="neirda24_tools.converter.array_to_text" class="Neirda24\Bundle\ToolsBundle\Converter\ArrayToText" />

        <service id="neirda24_tools.utils.array" class="Neirda24\Bundle\ToolsBundle\Utils\ArrayUtils" />
        <service id="neirda24_tools.utils.content" class="Neirda24\Bundle\ToolsBundle\Utils\ContentUtils" />
        <service id="neirda24_tools.utils.excel" class="Neirda24\Bundle\ToolsBundle\Utils\ExcelUtils" />
        <service id="neirda24_tools.utils.path" class="Neirda24\Bundle\ToolsBundle\Utils\PathUtils" />
        <service id="neirda24_tools.utils.string" class="Neirda24\Bundle\ToolsBundle\Utils\StringUtils" />
        <service id="neirda24_tools.utils.url" class="Neirda24\Bundle\ToolsBundle\Utils\UrlUtils">
            <argument type="service" id="neirda24_tools.utils.string" />
        </service>
    </services>
</container>
